{
    "parserOptions": {
        "sourceType": "module"
    },
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": "eslint:recommended",
    "plugins": ["compat"],
    "rules": {
        "indent": ["error", 2],
        "compat/compat": "error",
        "no-console": "off"
    }
}